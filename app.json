{
  "expo": {
    "name": "Navfinder",
    "slug": "Navfinder",
    "scheme": "navfinder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/OIG2.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/OIG2.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/OIG2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow NavFinder to use your location."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "80d12803-211f-478b-9dbe-0c77a5f8599a"
      }
    }
  }
}